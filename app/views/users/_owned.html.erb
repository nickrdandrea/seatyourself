<div class="owned" style="background:url(<%= restaurant.image %>")>
  <h2><%= restaurant.name %></h2>
  <p>Description: <%= restaurant.description %></p>
  <%= link_to "Edit", edit_restaurant_path(restaurant.id) %> |
  <%= link_to "Delete", restaurant_path(restaurant.id), method: :delete, data: {confirm: "Are you sure you want to Delete this restaurant?"} %>
</div>
